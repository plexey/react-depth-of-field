{
  "presets": [
    [
      "babel-preset-env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "> 3%"
          ]
        }
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    "babel-plugin-transform-react-jsx"
  ]
}